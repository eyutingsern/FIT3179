{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/eyutingsern/FIT3179/refs/heads/main/Graph%201.3%20Estimated%20resident%20population%20by%20country%20of%20birth(a)(b)%202.csv"
    },
    "transform": [
      {
        "fold": ["2013 (000)", "2018 (000)", "2023 (000)"],
        "as": ["Year", "Population"]
      }
    ],
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Year",
        "type": "ordinal",
        "title": "Year"
      },
      "y": {
        "field": "Population",
        "type": "quantitative",
        "title": "Population (000)"
      },
      "color": {
        "field": "Country",
        "type": "nominal",
        "legend": {"title": "Country of Birth"}
      },
      "tooltip": [
        {"field": "Country", "type": "nominal", "title": "Country"},
        {"field": "Year", "type": "ordinal", "title": "Year"},
        {"field": "Population", "type": "quantitative", "title": "Population (000)"}
      ]
    },
    "title": "Estimated Resident Population by Country of Birth (2013â€“2023)"
  }
  